{
  "name": "markdown-to-site",
  "version": "1.0.0",
  "description": "Markdown to site generator",
  "main": "index.js",
  "scripts": {
    "build": "nodemon node_modules/nodemon/bin/nodemon.js ./mdToHtml.js",
    "run": "node node_modules/http-server/bin/http-server -p 1235",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "ext": "css,html,js,md",
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "public/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rahulkadukar/markdown-to-site.git"
  },
  "author": "Rahul Kadukar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rahulkadukar/markdown-to-site/issues"
  },
  "homepage": "https://github.com/rahulkadukar/markdown-to-site#readme",
  "devDependencies": {
    "http-server": "^0.11.1",
    "markdown-it": "^8.4.2",
    "nodemon": "^1.18.11"
  }
}
