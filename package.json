{
  "name": "markdown-to-site",
  "version": "1.0.0",
  "description": "Markdown to site generator",
  "main": "index.js",
  "scripts": {
    "build": "node mdToHtml.js",
    "dev": "nodemon node_modules/nodemon/bin/nodemon.js ./mdToHtml.js",
    "run": "node node_modules/http-server/bin/http-server -p 1235",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "ext": "css,html,js,md",
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "public/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rahulkadukar/markdown-to-site.git"
  },
  "author": "Rahul Kadukar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rahulkadukar/markdown-to-site/issues"
  },
  "homepage": "https://github.com/rahulkadukar/markdown-to-site#readme",
  "devDependencies": {
    "assert": "^1.5.0",
    "dateformat": "^3.0.3",
    "fs-extra": "^7.0.1",
    "http-server": "^14.1.1",
    "js-yaml": "^3.14.1",
    "markdown-it": "^12.3.2",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-sup": "^1.0.0",
    "nodemon": "^2.0.20"
  }
}
